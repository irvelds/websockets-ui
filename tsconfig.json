      "noImplicitAny": true,
      "module": "commonjs",
      "target": "es2016",
      "rootDir": ".",
      "outDir": "dist",
      "allowJs": true,
      "esModuleInterop": true,
      "forceConsistentCasingInFileNames": true,           
